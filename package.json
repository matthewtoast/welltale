  "scripts": {
    "typecheck": "tsc --noEmit",
    "ts": "npx tsx --",
    "test": "for f in $(find . -name '*.test.ts' -not -path './node_modules/*'); do echo \"Running $f\"; npx tsx \"$f\" || exit 1; done",
    "repl": "yarn ts ./run/repl.ts",
    "auto": "yarn ts ./run/auto.ts",
    "web:dev": "next dev web",
    "web:typecheck": "tsc -p web/tsconfig.typecheck.json --noEmit",
    "ios:typecheck": "xcodebuild -project ios/Welltale/Welltale.xcodeproj -scheme Welltale -sdk iphonesimulator build 2>&1 | grep -E \"(error:|warning:)\"",
    "web:sst:deploy": "sst deploy --stage prod",
    "web:sst:remove": "sst remove --stage prod"
  },
